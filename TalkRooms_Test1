document.write('	 text-align: start;');
document.write('	 appearance: auto;');
document.write('	 -webkit-rtl-ordering: logical;');
document.write('	 resize: auto;');
document.write('	 cursor: pointer;');
document.write('	 white-space: pre-wrap;');
document.write('	 overflow-wrap: break-word;');
document.write('	 background-color: wheat;');
document.write('	 column-count: initial !important;');
document.write('	 box-sizing: border-box;');
document.write('	 margin: 0em;');
document.write('	 border-width: 0px;');
document.write('	 border-style: solid;');
document.write('	 border-color: -internal-light-dark(#767676, #858585);');
document.write('	 border-image: initial;');
document.write('	 padding: 2px;');
document.write('}');
document.write('/* https://developer.mozilla.org/en/docs/Web/CSS/box-shadow box-shadow: [inset?] [top] [left] [blur] [size] [color];');
document.write(' Tips: - We\'re setting all the blurs to 0 since we want a solid fill. - Add the inset keyword so the box-shadow is on the inside of the element - Animating the inset shadow on hover looks like the element is filling in from whatever side you specify ([top] and [left] accept negative values to become [bottom] and [right]) - Multiple shadows can be stacked - If you\'re animating multiple shadows, be sure to keep the same number of shadows on hover/focus as non-hover/focus (even if you have to create a transparent shadow) so the animation is smooth. Otherwise, you\'ll get something choppy. */');
document.write(' .fill:hover, .fill:focus {');
document.write('	 box-shadow: inset 0 0 0 2em var(--hover);');
document.write('}');
document.write(' .pulse:hover, .pulse:focus {');
document.write('	 animation: pulse 1s;');
document.write('	 box-shadow: 0 0 0 2em transparent;');
document.write('}');
document.write(' @keyframes pulse {');
document.write('	 0% {');
document.write('		 box-shadow: 0 0 0 0 var(--hover);');
document.write('	}');
document.write('}');
document.write(' .close:hover, .close:focus {');
document.write('	 box-shadow: inset -3.5em 0 0 0 var(--hover), inset 3.5em 0 0 0 var(--hover);');
document.write('}');
document.write(' .raise:hover, .raise:focus {');
document.write('	 box-shadow: 0 0.5em 0.5em -0.4em var(--hover);');
document.write('	 transform: translateY(-0.25em);');
document.write('}');
document.write(' .up:hover, .up:focus {');
document.write('	 box-shadow: inset 0 -3.25em 0 0 var(--hover);');
document.write('}');
document.write(' .slide:hover, .slide:focus {');
document.write('	 box-shadow: inset 6.5em 0 0 0 var(--hover);');
document.write('}');
document.write(' .offset {');
document.write('	 box-shadow: 0.3em 0.3em 0 0 var(--color), inset 0.3em 0.3em 0 0 var(--color);');
document.write('}');
document.write(' .offset:hover, .offset:focus {');
document.write('	 box-shadow: 0 0 0 0 var(--hover), inset 6em 3.5em 0 0 var(--hover);');
document.write('}');
document.write(' .fill {');
document.write('	 --color: #a972cb;');
