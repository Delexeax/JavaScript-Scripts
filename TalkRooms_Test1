document.write('<div class="Area">');
document.write('    <textarea id="TextForSendArea"></textarea>');
document.write('    <button class="close" onclick="javascript:send()">Send</button>');
document.write('</div>');
document.write('');
document.write('<style>');
document.write('    #TextForSendArea {');
document.write('	 font-family: monospace;');
document.write('	 text-rendering: auto;');
document.write('	 color: fieldtext;');
document.write('	 letter-spacing: normal;');
document.write('	 word-spacing: normal;');
document.write('	 line-height: normal;');
document.write('	 text-transform: none;');
document.write('	 text-indent: 0px;');
document.write('	 text-shadow: none;');
